{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    [
      "@babel/transform-react-jsx",
      {
        "pragma": "wp.element.createElement"
      }
    ]
  ],
  "env": {
    "default": {
      "plugins": ["transform-runtime"]
    }
  }
}
